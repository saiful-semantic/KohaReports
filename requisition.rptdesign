                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">Indenter</property>
                    <property name="nativeName">Indenter</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT 
	CONCAT(s.title, ' / ', s.author, ' ', s.isbn) AS 'Title/Author/ISBN No.',
	CONCAT(s.currency,' ', ROUND(s.price,2)) as 'Cost (approx)', 
	s.quantity AS 'Qty', 
    s.STATUS AS 'Status',
    s.suggesteddate AS 'Suggested_Date',
	CONCAT(b.firstname,' ',b.surname) as 'Indenter'
FROM 
	suggestions s
	LEFT JOIN borrowers b ON (s.suggestedby = b.borrowernumber)
WHERE s.STATUS = 'CHECKED'
	AND b.cardnumber = ?]]></xml-property>
